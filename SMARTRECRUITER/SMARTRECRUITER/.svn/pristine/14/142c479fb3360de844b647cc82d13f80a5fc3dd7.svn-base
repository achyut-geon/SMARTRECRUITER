function CheckInteger(e) {
    var t = e.keyCode ? e.keyCode : e.charCode ? e.charCode : e.which;
    8 != t && 9 != t && (48 > t || t > 57) && (e.preventDefault ? e.preventDefault() : e.returnValue = !1)
}

