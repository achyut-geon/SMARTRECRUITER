                        <input type="text" id="txtCaptcha" class="captchabox" disabled />
                    </div>
                    <div class="re_btn1">
                        <button type="button" id="btnrefresh" class="refreh_btn" ng-click="captchain = null" onclick="ObjUserLogin.DrawCaptcha();">
